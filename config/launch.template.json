{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "nodemon",
            "runtimeExecutable": "nodemon",
            "program": "${workspaceFolder}/src/server/app.js",
            "restart": true,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "env": {
                "NODE_ENV": "dev",
                "PORT": "3000",
                "DATA_BASE_URL": "mongodb://localhost:27017/udemy-nodejs-cafe",
                "TOKEN_EXPIRED_IN": "7d",
                "TOKEN_PRIVATE_KEY": "???",
                "CLIENT_ID": "??"
            }
        }
    ]
}